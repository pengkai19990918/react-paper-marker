{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "antd-demo": ["src"],
      "antd-demo/*": ["src/*", "*"],
      "react-paper-bindings": ["../src"]
    },
    "typeRoots": ["types", "node_modules/@types"]
  },
  "include": [
    ".dumirc.ts",
    "src/**/*",
    "../src/**/*.ts",
    "../src/**/*.tsx"
  ]
}
